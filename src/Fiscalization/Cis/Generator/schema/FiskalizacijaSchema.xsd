					<element name="RacunPD" type="tns:RacunPDType" minOccurs="1"
						maxOccurs="1" />
				</choice>	
				<element ref="ds:Signature" minOccurs="0" maxOccurs="1" />
			</sequence>
			<attribute name="Id" type="string" />
		</complexType>
	</element>

	<element name="ProvjeraOdgovor">
		<annotation>
